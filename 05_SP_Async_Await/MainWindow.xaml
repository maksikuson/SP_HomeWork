<Window x:Class="_05_SP_Async_Await.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_05_SP_Async_Await"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <ToolBar DockPanel.Dock="Top" Padding="10">

            <Button Click="Button_Click_1">Select File</Button>
            <Button Click="Button_Click_2">Select Folder</Button>
            <Button Click="Button_Click_3">Copy</Button>
            <TextBox x:Name="from" Width="250"></TextBox>
            <TextBox Name="to" Width="250"></TextBox>
            <Button Click="Button_Click_4">OK</Button>
        </ToolBar>
        <ListBox x:Name="list"></ListBox>
    </DockPanel>
</Window>
